<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>WarframeNexus - Trade Prices</title>

    <link href="css/importer.css" rel="stylesheet" type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway:300,500,600,700' rel='stylesheet' type='text/css'>

    <script src="js/jquery.min.js"></script>
    <script src="js/gallery/motionblur.js"></script>
    <script src="js/gallery/gallery.js"></script>
    <script src="js/gallery/gallery_recent.js"></script>
    <script src="js/gallery/TweenMax.min.js"></script>

    <script src="js/parallax.js"></script>
    <script src="js/index.js"></script>
</head>




<div class='lp-bg' data-parallax="scroll" data-image-src="img/lp_bg.jpg"></div>




<% include .\templates\nav %>



<!-- Header -->
<!--==============================-->

<header>
    <div class='generic-header'>
        <div class='generic-header-headline-container'>
            <h1 class='generic-header-headline'><%= page_title %></h1>
        </div>
    </div>
</header>






<!-- Content -->
<!--==============================-->

<div class='generic-content'>
    <div class='main-content-wrapper'>
        <h1><%= main_title %></h1>
        <% content.content_comps.forEach(function(item) { %>
           <h2><%= item.Title %></h2>
           <span>
           <%- item.Desc %>
           </span>
        <% }); %>
    </div>
</div>




<% include .\templates\footer %>


</html>
